{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "ng2-daterangepicker": {
      "root": "projects/ng2-daterangepicker",
      "sourceRoot": "projects/ng2-daterangepicker/src",
      "projectType": "library",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/ng2-daterangepicker/tsconfig.lib.json",
            "project": "projects/ng2-daterangepicker/ng-package.json"
          }
        }
      }
    },
    "ci": {
      "root": "projects/ci-app",
      "sourceRoot": "projects/ci-app/src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist",
            "index": "projects/ci-app/src/index.html",
            "main": "projects/ci-app/src/main.ts",
            "tsConfig": "projects/ci-app/src/tsconfig.app.json",
            "polyfills": "projects/ci-app/src/polyfills.ts",
            "assets": [
              "projects/ci-app/src/assets",
              "projects/ci-app/src/favicon.ico"
            ],
            "styles": [
              "projects/ci-app/src/styles.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "fileReplacements": [
                {
                  "replace": "projects/ci-app/src/environments/environment.ts",
                  "with": "projects/ci-app/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "ci:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "ci:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "ci:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/ci-app/src/test.ts",
            "karmaConfig": "./karma.conf.js",
            "polyfills": "projects/ci-app/src/polyfills.ts",
            "tsConfig": "projects/ci-app/src/tsconfig.spec.json",
            "scripts": [],
            "styles": [
              "projects/ci-app/src/styles.css"
            ],
            "assets": [
              "projects/ci-app/src/assets",
              "projects/ci-app/src/favicon.ico"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/ci-app/src/tsconfig.app.json",
              "projects/ci-app/src/tsconfig.spec.json"
            ],
            "exclude": []
          }
        }
      }
    },
    "ci-e2e": {
      "root": "e2e",
      "sourceRoot": "e2e",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "./protractor.conf.js",
            "devServerTarget": "ci:serve"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "e2e/tsconfig.e2e.json"
            ],
            "exclude": []
          }
        }
      }
    }
  },
  "defaultProject": "ci",
  "schematics": {
    "@schematics/angular:component": {
      "prefix": "app",
      "styleext": "css"
    },
    "@schematics/angular:directive": {
      "prefix": "app"
    }
  }
}
